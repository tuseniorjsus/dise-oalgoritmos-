Proceso MayorValor 
	A<-0;
	B<-0;
	
	Mientras A==B Hacer
		Escribir "Escribe el valor de la primera variable(Introduce valores diferentes)";
		Leer A ;
		Escribir "Escribe el valor de la segunda variable(Introduce valores diferentes)" ;
		Leer B;
	Fin Mientras;
	Si A>B Entonces
		Escribir "El valor de la primera variable es mayor a la segunda"; 
	SiNo
		Escribir "El valor de la segunda variable es mayor a la primera";
	Fin Si
	
FinProceso
